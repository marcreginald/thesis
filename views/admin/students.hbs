{{> admin/head}}

<body class="animsition">
    <div class="page-wrapper">


        {{> admin/sidebar}}

        <!-- PAGE CONTAINER-->
        <div class="page-container">
            <!-- HEADER DESKTOP-->
            {{> admin/navbar}}
            <!-- HEADER DESKTOP-->

            <!-- MAIN CONTENT-->
            <div class="main-content">




                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-md-12">
                                <!-- DATA TABLE -->
                                <div class="table-data__tool">
                                    <div class="table-data__tool-left">

                                        <h3 class="title-5 m-b-35">Student Table</h3>
                                        <div class="rs-select2--light rs-select2--sm">
                                        </div>

                                    </div>
                                    <div class="table-data__tool-right">
                                        <a href="/add-student">
                                            <button class="au-btn au-btn-icon au-btn--green au-btn--small">
                                                <i class="zmdi zmdi-plus"></i>add item</button>
                                        </a>

                                    </div>
                                </div>
                                <div class="table-responsive table-responsive-data2">
                                    <table class="table table-data2">
                                        <thead>
                                            <tr>
                                                <th>
                                                </th>
                                                <th>Name</th>
                                                <th>Email</th>
                                                <th>Gender</th>
                                                <th>Birth Date</th>
                                                <th>Address</th>
                                                <th>Assessment Center</th>
                                                <th>Specialization</th>
                                                <th>status</th>
                                                <th></th>
                                            </tr>
                                        </thead>

                                        {{#each student}}
                                        <tbody>
                                            <tr class="tr-shadow">
                                                <td>
                                                    <label class="au-checkbox">
                                                        <input type="checkbox">
                                                        <span class="au-checkmark"></span>
                                                    </label>
                                                </td>
                                                <td>{{lastName}} {{firstName}}, {{middleName}}</td>
                                                <td>
                                                    <span class="block-email">{{email}}</span>
                                                </td>
                                                <td class="desc">{{gender}}</td>
                                                <td>{{birthDate}}</td>
                                                <td>
                                                    {{address}}
                                                </td>
                                                <td>{{school}}</td>
                                                <td>{{nc}}</td>
                                                <td>
                                                    <span class="status--process">{{status}}</span>

                                                </td>
                                                <td>
                                                    <div class="table-data-feature">
                                                        <button class="item" data-toggle="tooltip" data-placement="top"
                                                            title="Edit">
                                                            <i class="zmdi zmdi-edit"></i>
                                                        </button>
                                                        <button class="item" data-toggle="tooltip" data-placement="top"
                                                            title="Delete">
                                                            <i class="zmdi zmdi-delete"></i>
                                                        </button>

                                                    </div>
                                                </td>
                                            </tr>


                                        </tbody>
                                        {{/each}}
                                    </table>
                                </div>
                                <!-- END DATA TABLE -->
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-12">
                                <div class="copyright">
                                    <p>Copyright Â© 2018 Group28. All rights reserved.</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- END MAIN CONTENT-->
            <!-- END PAGE CONTAINER-->
        </div>

    </div>

    {{> admin/footer}}