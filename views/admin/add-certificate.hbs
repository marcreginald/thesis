{{> admin/head}}

<body class="animsition">
    <div class="page-wrapper">


        {{> admin/sidebar}}

        <!-- PAGE CONTAINER-->
        <div class="page-container">
            <!-- HEADER DESKTOP-->
            {{> admin/navbar}}
            <!-- HEADER DESKTOP-->

            <!-- MAIN CONTENT-->
            <div class="main-content">

                <div class="col-lg-12">
                    <div class="card">
                        <div class="card-header">
                            <strong>Tesda Accredited Assessment Centers</strong>
                            <small> Form</small>
                        </div>
                        <div class="card-body card-block">
                            {{#if errorMessage}}
                            <div class="alert alert-danger" role="alert">
                                {{errorMessage}}
                            </div>
                            {{/if}}
                            <form action="/certificate-holder-add" method="POST" class="form-horizontal">
                                <div class="form-group">
                                    <label for="certificateHolderName" class=" form-control-label">Certificate Holder
                                        Name</label>
                                    <select name="certificateHolderName" id="certificateHolderName"
                                        class="form-control">
                                        <option value="0">Select Student</option>
                                        {{#each student}}
                                        <option value="{{lastName}}, {{firstName}}
                                            {{middleName}}">{{lastName}}, {{firstName}}
                                            {{middleName}}</option>
                                        {{/each}}
                                    </select>
                                </div>
                                <div class="form-group">
                                    <label for="specialization" class=" form-control-label">Specialization</label>
                                    <input type="text" id="specialization disabled-input"
                                        name="specialization disabled-input" value="{{#each student}}{{nc}}{{/each}}"
                                        disabled="" class="form-control">
                                </div>
                                <div class="form-group">
                                    <label for="certificateType" class=" form-control-label">Certificate Type</label>
                                    <select name="certificateType" id="certificateType" class="form-control">
                                        <option value="0">Select Certificate Type</option>
                                        <option value="1">NC-I</option>
                                        <option value="2">NC-II</option>
                                        <option value="3">NC-III</option>
                                        <option value="4">NC-IV</option>
                                        <option value="5">COC</option>
                                    </select>
                                </div>
                                <div class="form-group">
                                    <label for="centerName" class=" form-control-label">Center Name</label>
                                    <input type="text" id="centerName disabled-input" name="centerName disabled-input"
                                        value="{{#each student}}{{school}}{{/each}}" disabled="" class="form-control">
                                </div>
                                <div class="form-group">
                                    <label for="issuedDate" class=" form-control-label">Issued Date</label>
                                    <input type="text" id="issuedDate" name="issuedDate" placeholder="MM/DD/YYYY"
                                        class="form-control">
                                </div>
                                <div class="form-group">
                                    <label for="expiryDate" class=" form-control-label">Expiry Date</label>
                                    <input type="text" id="expiryDate" name="expiryDate" placeholder="MM/DD/YYYY"
                                        class="form-control">
                                </div>
                                <div class="form-group">
                                    <button type="submit" class="btn btn-primary btn-sm">
                                        <i class="fa fa-dot-circle-o"></i> Submit
                                    </button>
                                    <button type="reset" class="btn btn-danger btn-sm">
                                        <i class="fa fa-ban"></i> Reset
                                    </button>
                                </div>
                            </form>

                        </div>

                    </div>
                    <br />

                </div>

            </div>


        </div>
        <!-- END MAIN CONTENT-->
        <!-- END PAGE CONTAINER-->
    </div>

    </div>

    {{> admin/footer}}